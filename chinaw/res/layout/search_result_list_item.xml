<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/search_result_listitem_titleAndScore"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:focusable="false"
        android:gravity="center_vertical"
        android:text="无锡景（无锡民歌） （关联度：99%）"
        android:textSize="15sp" 
        />

    <WebView
        android:id="@+id/search_result_listitem_webview"
        android:layout_width="match_parent"
        android:layout_height="60dp" />

    <TextView
        android:id="@+id/search_result_listitem_link"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:focusable="false"
        android:gravity="center_vertical"
        android:text="http://www.wuxi.gov.cn//wap/mlxc/wszg/wxryz/5893866.shtml"
        android:textSize="10sp" />

</LinearLayout>